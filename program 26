using System;

class Fibonacci
{
    static void Main()
    {
        Console.WriteLine("Enter number of terms:");
        int n = Convert.ToInt32(Console.ReadLine());

        int a = 0, b = 1;
        Console.WriteLine("Fibonacci Series:");

        for (int i = 0; i < n; i++)
        {
            Console.WriteLine(a);
            int next = a + b;
            a = b;
            b = next;
        }
    }
}
